import java.util.Scanner;

class Main {
  public static void main(String[] args) {
	  Scanner sc = new Scanner(System.in);
	  String S = sc.nextLine();
	  int length = S.length();
	  String revStr = "";
	  for(int i=length-1;i>=0;i--){
		  revStr = revStr+S.charAt(i);
	  }
	  System.out.println(revStr);
  }
}
